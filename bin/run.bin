VERSION=0.7.1
HELP_DIR=../swallow-web/src/main/webapp/help

gen(){
	 sed -i 's/<body>\(.*\)/<body>\n\1/' $1
	 sed -i '/<body>/r div' $1
	 sed -i '$r js' $1
}

put(){
	mv $1 $2
	open $2
}


gen ../README.html 
put ../README.html $HELP_DIR/help_$VERSION.html


gen ../UPGRADE.html 
put ../UPGRADE.html $HELP_DIR/upgrade.html
